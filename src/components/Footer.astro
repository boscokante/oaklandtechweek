      <!-- Quick Links -->
      <div>
        <h4 class="text-lg font-semibold text-white mb-4">Quick Links</h4>
        <ul class="space-y-2">
          <li><a href="/speakers" class="text-slate-300 hover:text-white transition-colors">Speakers</a></li>
